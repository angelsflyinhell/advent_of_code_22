    "score = 0;\n",
    "with open('./input.txt') as f:\n",
    "    for line in f:\n",
    "        ar, al, br, bl = line.strip().replace(',', '-').split('-')\n",
    "        if(int(al) >= int(bl) and int(ar) <= int(br) or int(al) <= int(bl) and int(ar) >= int(br)):\n",
    "            score += 1\n",
    "\n",
    "print(score)"
   ]
  }
